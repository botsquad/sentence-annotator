{
  "name": "intent-annotator",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/arjan/intent-annotator.git"
  },
  "description": "A simple Webpack 4 starter with TypeScript transpilation",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/ --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "test": "karma start karma.config.js",
    "test-dev": "TEST_MODE=true karma start karma.config.js"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "Juri Strumpflohner",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "2.8.7",
    "@types/node": "7.0.0",
    "ava": "^2.4.0",
    "jasmine-core": "3.1.0",
    "karma": "^4.4.1",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.0",
    "source-map-loader": "0.2.3",
    "tslint": "5.10.0",
    "tslint-loader": "3.6.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.22.3",
    "@types/classnames": "^2.2.9",
    "@types/lodash": "^4.14.117",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "classnames": "^2.2.6",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  }
}
